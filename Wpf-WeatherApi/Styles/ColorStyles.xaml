<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    
    
    <LinearGradientBrush x:Key="cs_Background" StartPoint="0,0" EndPoint="1,2" Opacity="0.70">
        <GradientStop Color="#FF000000" Offset="0.0"/>
        <GradientStop Color="#FF000000" Offset="1.0"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="cs_BorderBrush" Color="#FF040F42"/>

    <SolidColorBrush x:Key="cs_BBackground" Color="White" Opacity="0.5"/>

    <SolidColorBrush x:Key="cs_Text" Color="#FFE2DD00"/>

    <SolidColorBrush x:Key="cs_Text2" Color="#FFECEBA9"/>

    <SolidColorBrush x:Key="cs_Text3" Color="White"/>

    <SolidColorBrush x:Key="cs_TxtBBrush" Color="#FF086161"/>

    
</ResourceDictionary>